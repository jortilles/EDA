{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@eda/components/*": [
        "app/module/components/*"
      ],
      "@eda/services/*": [
        "app/services/*"
      ],
      "@eda/models/*": [
        "app/shared/models/*"
      ],
      "@eda/pipes/*": [
        "app/shared/pipes/*"
      ],
      "@eda/shared/*": [
        "app/shared/*"
      ],
      "@eda/configs/*": [
        "app/config/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "useDefineForClassFields": false
  }
}
